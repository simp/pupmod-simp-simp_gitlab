{
  "name": "simp-simp_gitlab",
  "version": "0.1.0",
  "author": "SIMP Team",
  "summary": "SIMP profiles for GitLab",
  "license": "Apache-2.0",
  "source": "https://github.com/simp/pupmod-simp-simp_gitlab",
  "project_page": "https://github.com/simp/pupmod-simp-simp_gitlab",
  "issues_url": "https://simp-project.atlassian.net",
  "tags": [
    "simp",
    "gitlab",
    "ci",
    "gitlab-ce",
    "gitlab-omnibus"
  ],
  "dependencies": [
    {
      "name": "simp/iptables",
      "version_requirement": ">= 6.0.1 < 7.0.0"
    },
    {
      "name": "simp/ntpd",
      "version_requirement": ">= 6.0.0 < 7.0.0"
    },
    {
      "name": "simp/pam",
      "version_requirement": ">= 6.0.0 < 7.0.0"
    },
    {
      "name": "simp/postfix",
      "version_requirement": ">= 5.0.0 < 6.0.0"
    },
    {
      "name": "simp/simplib",
      "version_requirement": ">= 3.3.1 < 4.0.0"
    },
    {
      "name": "simp/svckill",
      "version_requirement": ">= 3.2.1 < 4.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.13.1 < 5.0.0"
    },
    {
      "name": "vshn/gitlab",
      "version_requirement": ">= 1.13.3 < 2.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.6.0 < 5.0.0"
    }
  ],
  "data_provider": "hiera"
